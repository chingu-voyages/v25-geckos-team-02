{
    "rules": {
      "no-console": 1,
      "no-unused-vars": 1
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "prettier"
    ],
    "plugins": ["import"]
  }
  